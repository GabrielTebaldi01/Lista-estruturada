/*exercicio 1*/

#include<stdio.h>
#include<stdlib.h>
#include<math.h>


int maior(){
    
	
	int x[2],i;
    
	
	for ( i = 0; i < 2; i++){
         printf("Me informe o numero:");
         scanf("%d",&x[i]);
    }
    
	
	if (x[0] > x[1])
    {
       printf("Esse e o maior numero %d",x[0]);
    }
	
	else if (x[1] > x[0])
    {
        printf("Esse e o maior numero %d",x[1]);
    }
	
	else
	{
        printf("Iguais");
    }
    
    
    
}
int main (){
    maior();
    
    return 0;
}


/*exercicio 2*/


#include<stdio.h>
#include<stdlib.h>
#include<math.h>


int printMes (int mes){
    switch (mes)
    {
        case 1:
            printf("Janeiro");
            break;
        case 2:
            printf("Fevereiro");
            break;
        case 3:
            printf("marco");
            break;
        case 4:
            printf("abriu");
            break;
        case 5:
            printf("Maio");
            break;
        case 6:
            printf("Junho ");
            break;
        case 7:
            printf("Julho");
            break;
        case 8:
            printf("agosto");
            break;
        case 9:
            printf("setembro");
            break;
        case 10:
            printf("Outubro");
            break;
        case 11:
            printf("Novembro");
            break;
        case 12:
            printf("Dezembro");
            break;
        default:
            printf("Mes nao fornecido");
            break;
    }
}
int main (){
    int mes ;
    printf("Informe o mes:");
    scanf("%d",&mes);
    printMes(mes);
}


/*exercicio 3*/


#include<stdio.h>
#include<stdlib.h>
#include<math.h>


int conversao(double tf)
{
    double tc;
tc = (tf - 32) * (5.0/9.0);
   
   printf("A temperatura em Celsius e:%.2lf",tc);
}



int main(){
    double tf ;
    
	printf("Me forneca a temperatura em Fahrenheit:");
    scanf("%lf",&tf);
    
	 conversao(tf);
}


/*exercicio 4*/



#include<stdio.h>
#include<stdlib.h>
#include<math.h>


int volume(float h,float r){
float volume;


 volume = 3.14 * pow(r,2)*h;

printf("Volume=  %.2f\n\n",volume);
}


int main (){
    float h ,r;
    
	printf("Me informe a altura");
    scanf("%f",&h);
    
    printf("Me informe o raio");
    scanf("%f",&r);
    
	volume(h,r);
}


/*exercicio 5*/


#include<stdio.h>
#include<stdlib.h>
#include<math.h>


int volume(float r){

float volume;
 
 volume = 4/((3*3.14)*pow(r,3));

 printf("Volume = %.2f",volume);
}


int main (){
    float r;
    
    printf("Me informe o raio:");
    scanf("%f",&r);
    
	volume(r);
}


