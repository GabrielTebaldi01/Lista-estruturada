/*exercicio numero 1*/


#include  <stdio.h>
#include <math.h>
#include<time.h>
#include <stdlib.h>

int main(){

int nota[30],i;
    
	
	
	for (i=0;i<=30;i++){
        printf("Informe a nota do aluno %d :",i+1);
        scanf("%d\n\n",&nota[i]);
    }
    
}



/*exercicio numero 2*/


#include  <stdio.h>
#include <math.h>
#include<time.h>
#include <stdlib.h>

int main(){
	
int numeroA;
    printf("Informe a quantidade de alunos avaliados :");
    scanf( "%d",&numeroA);//perguntar pq se as variaveis a baixo estiverem em cima nao roda


int nota[numeroA],i;
    
	
	
	for (i=0;i<=numeroA-1;i++){
        printf("Informe a nota do aluno %d :",i+1);
        scanf("%d\n\n",&nota[i]);
    }
    
}


